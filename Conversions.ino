float knotsToKph(float knots) {
  return (knots * 1.852);
}

float kphToKnots(float kph) {
  return (kph * 0.539957);
}

float knotsToMph(float knots) {
  return (knots * 1.15078);
}

float mphToKph(float mph) {
  return (mph * 1.60934);
}

float kphToMph(float kph) {
  return (kph * 0.621371);
}

float feetToMeters(float feet) {
  return (feet * 0.3048);
}

float metersToFeet(float meters) {
  return (meters * 3.28084);
}

